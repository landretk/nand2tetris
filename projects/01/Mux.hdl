/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel);
    And(a=a, b=notSel, out=out1a);
    And(a=b, b=sel, out=out1b);
    Or(a=out1a, b=out1b, out=out);
}